<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:CoinBase.Styles">

    <Style TargetType="Line" x:Key="ChartGridColor">
        <Setter Property="Stroke"		   Value="{ThemeResource TextBoxForegroundHeaderThemeBrush}"/>
        <Setter Property="StrokeDashArray" Value="3,7"/>
        <Setter Property="StrokeThickness" Value="0.2"/>
    </Style>

    <Style TargetType="Line" x:Key="TransparentLine">
        <Setter Property="Stroke" Value="{ThemeResource SystemControlPageBackgroundChromeLowBrush}"/>
    </Style>
    <DataTemplate x:Key="TransparentRect">
        <Rectangle Stroke="Transparent"/>
    </DataTemplate>

</ResourceDictionary>
